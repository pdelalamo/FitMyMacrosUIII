{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@env": [
        "node_modules/react-native-dotenv"
      ]
    },
    "jsx": "react-native",
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "extends": "expo/tsconfig.base"
}